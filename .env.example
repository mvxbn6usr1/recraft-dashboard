# Application
NODE_ENV=development
API_VERSION=v1
PORT=3000

# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/app_production?schema=public"
TEST_DATABASE_URL="postgresql://postgres:postgres@localhost:5432/app_test?schema=public"

# Authentication
JWT_SECRET=your-super-secret-jwt-key
JWT_EXPIRES_IN=7d

# Frontend
VITE_API_URL=http://localhost:3000/api/v1
VITE_APP_NAME="Recraft Dashboard"

# External Services
OPENAI_API_KEY=your-openai-api-key

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=debug
LOG_FORMAT=dev 